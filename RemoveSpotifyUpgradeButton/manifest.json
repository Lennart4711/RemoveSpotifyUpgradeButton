{
  "manifest_version": 2,
  "name": "RemoveSpotifyUpgradeButton",
  "version": "1.0",
  "description": "Removes the pesky spotify upgrade button",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.spotify.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}